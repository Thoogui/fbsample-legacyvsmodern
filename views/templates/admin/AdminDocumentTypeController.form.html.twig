{% extends '@PrestaShop/Admin/layout.html.twig' %}

{% trans_default_domain "Module.fbsample_legacyvsmodern.Admin" %}

{% block content %}

<div class="row">
  <div class="col-lg-8">
    <div class="card">
      <h3 class="card-header">
        <i class="material-icons">edit</i> {{ 'Edit document type'|trans({}, 'Module.fbsample_legacyvsmodern.Admin') }}
      </h3>
      <div class="card-block">
        {{ form(AdminDocumentTypeForm) }}
      </div>
    </div>
  </div>
</div>

{% endblock %}

{% block stylesheets %}
  {{ parent() }}

  <link rel="stylesheet" href="{{ asset('../modules/fbsample_legacyvsmodern/views/assets/css/admindocumenttypecontroller.css') }}">
{% endblock %}


{% block javascripts %}
  {{ parent() }}
  <script src="{{ asset('../modules/fbsample_legacyvsmodern/views/assets/js/admindocumenttypecontroller.js') }}"></script>
{% endblock %}


